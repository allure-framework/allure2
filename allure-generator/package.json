    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.7",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "file-loader": "^6.2.0",
    "handlebars-jest": "^0.5.0",
    "handlebars-loader": "^1.7.1",
