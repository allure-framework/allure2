<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xsd="http://www.w3.org/2001/XMLSchema"
               schemaLocation="../allure_report-data.xsd"
               jaxb:version="2.1">

    <jaxb:bindings node="//xsd:complexType[@name='Time']">
        <jaxb:class ref="org.allurefw.report.entity.Time"/>
    </jaxb:bindings>

    <jaxb:bindings node="//xsd:complexType[@name='GroupTime']">
        <jaxb:class ref="org.allurefw.report.entity.GroupTime"/>
    </jaxb:bindings>

    <jaxb:bindings node="//xsd:complexType[@name='TestCaseInfo']">
        <jaxb:class ref="org.allurefw.report.entity.TestCaseInfo"/>
    </jaxb:bindings>

    <jaxb:bindings node="//xsd:complexType[@name='Status']">
        <jaxb:class ref="org.allurefw.report.entity.Status"/>
    </jaxb:bindings>

    <jaxb:bindings node="//xsd:complexType[@name='SeverityLevel']">
        <jaxb:class ref="org.allurefw.report.entity.SeverityLevel"/>
    </jaxb:bindings>

    <jaxb:bindings node="//xsd:complexType[@name='Failure']">
        <jaxb:class ref="org.allurefw.report.entity.Failure"/>
    </jaxb:bindings>

    <jaxb:bindings node="//xsd:complexType[@name='Statistic']">
        <jaxb:class ref="org.allurefw.report.entity.Statistic"/>
    </jaxb:bindings>

</jaxb:bindings>
