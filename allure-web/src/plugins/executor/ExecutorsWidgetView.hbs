<h2 class="widget__title">
    Executors
</h2>
<div class="table table_hover widget__table">
{{#if items}}
    {{#each items}}
    <span class="table__row">
        <div class="{{b 'table' 'col'}} executor">
            <span class="executor-icon executor-icon__{{info.type}}">&nbsp;</span>
            {{name}}
        </div>
        {{#if info}}
            <div class="{{b 'table' 'col' right=true}} executor">
                <a class="link" href="{{info.buildUrl}}">
                    {{info.buildName}}
                    <span class="fa fa-external-link"></span>
                </a>
            </div>
        {{else}}
            <div class="{{b 'table' 'col' right=true}}">
                Unknown
            </div>
        {{/if}}
    </span>
    {{/each}}
{{else}}
    <div class="table__row">
        <div class="{{b 'table' 'col' center=true}}">
            There are no information about tests executors
        </div>
    </div>
{{/if}}
</div>
